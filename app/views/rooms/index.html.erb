<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
       <th>Room</th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%=h room.name %></td>
    <td><%=h room.description %></td>
     <td><%=h room.capacity %></td>
    <td><%= link_to 'Show', room %></td>
    
  </tr>
<% end %>
</table>

<br />
