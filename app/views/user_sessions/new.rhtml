<h1>Login</h1>
<% form_for @user_session do |form| %>
 <%= form.error_messages %>



   1. <%= form.label :username, "Username" %> <%= form.text_field :username %><br />
   2. <%= form.label :password, "Password" %> <%= form.password_field :password %><br />

 <%= form.check_box :remember_me %><%= form.label :remember_me %><br />

    <%= form.submit "Submit" %>
<% end %>

