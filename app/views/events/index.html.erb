<h1>Listing events</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Address</th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%=h event.name %></td>
    <td><%=h event.description %></td>
    <td><%=h event.address %></td>
    <td><%= link_to 'Show', event %></td>
   
  </tr>
<% end %>
</table>

<br />
